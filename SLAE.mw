<Group hide-input="false" labelreference="L3" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">
Seidel := proc(A::Matrix, b::Vector, eps::numeric := 1e-6, maxIter::integer := 1000)
    local n, x, x_old, i, j, iter, sum;
    n := RowDimension(A);
    x := Vector(n, 0.0);
    iter := 0;
    
    while iter &lt; maxIter do
        x_old := x;
        for i from 1 to n do
            sum := 0.0;
            for j from 1 to n do
                if j &lt;&gt; i then
                    sum := sum + A[i, j] * x[j];
                end if;
            end do;
            x[i] := (b[i] - sum) / A[i, i];
        end do;
        iter := iter + 1;
        if Norm(x - x_old, infinity) &lt; eps then
            break;
        end if;
    end do;
    print(iter);
    return x;
end proc:
</Text-field>
</Input>
</Group>
