<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">with(LinearAlgebra):

SimpleIter := proc(A::Matrix, b::Vector, x0::Vector, tol::numeric, maxIter::integer)
    local n, d, d_inv, D, Dinv, R, x_old, x_new, k, err, i;
    
    n := RowDimension(A);
    d     := [ seq( A[i,i],   i=1..n ) ];
    d_inv := [ seq( 1/A[i,i], i=1..n ) ];

    D    := DiagonalMatrix(d);
    Dinv := DiagonalMatrix(d_inv);
    
    R := A - D;
    
    x_old := x0;
    for k from 1 to maxIter do
        x_new := Dinv . (R . x_old + b);
        err   := Norm(x_new - x_old, infinity);
        if err &lt; tol then
            return evalf(x_new);
        end if;
        x_old := x_new;
    end do;
    
<Font encoding="UTF-8">    printf(&quot;\320\235\320\265 \320\264\320\276\321\201\321\202\320\270\320\263\320\275\321\203\321\202\320\260 \321\202\320\276\321\207\320\275\320\276\321\201\321\202\321\214 %g \320\267\320\260 %d \320\270\321\202\320\265\321\200\320\260\321\206\320\270\320\271, \320\277\320\276\321\201\320\273\320\265\320\264\320\275\321\217\321\217 \320\277\320\276\320\263\321\200\320\265\321\210\320\275\320\276\321\201\321\202\321\214 = %g\134n&quot;,
