<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">with(LinearAlgebra):

Gauss := proc(f, a, b, n)
    local i, J, E, nodes, vecs, weights, total, x, h, c, v1;
    h := (b - a)/2;
    c := (a + b)/2;
    
    J := Matrix(n, n, 0);
    for i from 1 to n-1 do
        J[i, i+1] := i / sqrt(4*i^2 - 1);
        J[i+1, i] := i / sqrt(4*i^2 - 1);
    end do;
    
    E := Eigenvectors(J, output = 'list');
    nodes := map(e -&gt; e[1], E);
    vecs := map(e -&gt; e[3], E);
    v1 := map(u -&gt; u[1], vecs);
    
    weights := Vector(n);
    for i from 1 to n do
        weights[i] := 2 * (v1[i][1])^2;
    end do;
    
    total := 0;
    for i from 1 to n do
        x := h * nodes[i] + c;
        total := total + weights[i] * f(x);
    end do;
    
    return evalf(h * total);
end proc:</Text-field>
