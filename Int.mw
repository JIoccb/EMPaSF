<Group labelreference="L16" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Simpson := proc(f, a, b, n)
  local h, res, i, x;
  if n mod 2 &lt;&gt; 0 then
    error &quot;n must be even&quot;;
  end if:
  h := (b - a) / n:
  res := evalf(f(a)) + evalf(f(b)):
  for i from 1 to n-1 do
    x := a + i * h:
    if i mod 2 = 0 then
      res := res + 2 * evalf(f(x)):
    else
      res := res + 4 * evalf(f(x)):
    end if:
  od:
  res := res * h / 3:
  return res:
end proc:
</Text-field>
</Input>
</Group>
