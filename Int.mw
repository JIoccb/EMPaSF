<Group hide-input="false" labelreference="L2" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Rectangle := proc(f, a, b, n)
  local res, h, i, x;
  res := 0:
  h := (b - a) / n:
  for i from 1 to n do
    x := a + (i - 1/2) * h:
    res := res + f(x):
  od:
  res := res * h:
  return evalf(res):
end proc:</Text-field>
</Input>
