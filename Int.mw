<Group hide-input="false" labelreference="L7" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Trapeze := proc(f, a, b, n)
  local res, h, i, x;
  h := (b - a) / n:
  res := evalf(f(a)) + evalf(f(b)):
  for i from 1 to n-1 do
    x := a + i * h:
    res := res + 2 * evalf(f(x)):
  od:
  res := res * h / 2:
  return res:
end proc:</Text-field>
</Input>
